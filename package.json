{
  "name": "dotenv2env",
  "version": "1.0.3",
  "description": "Loads the environment variables from a .env file",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "dotenv2env": "dist/index.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "commit": "git-cz",
    "lint": "eslint './src/*.ts'",
    "lint:fix": "eslint './src/*.ts' --fix",
    "start": "node ./dist/lib/index.js power --clear --android --useVerbose",
    "power": "node --version",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "semantic-release": "semantic-release --branches main",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "author": "Carlos Gabriel Godri",
  "license": "MIT",
  "dependencies": {
    "chalk": "4.1.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "semantic-release": "^21.0.1",
    "ts-jest": "^29.1.0",
    "ts-standard": "^12.0.2",
    "typescript": "5.0.4"
  }
}
